{
  "name": "@achengyu/vconsole",
  "license": "MIT",
  "version": "0.0.5",
  "author": "chengyu",
  "description": "a better console tool for web development",
  "main": "./dist/vconsole.umd.js",
  "module": "./dist/vconsole.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vconsole.es.js",
      "require": "./dist/vconsole.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/a1067111756/console.git"
  },
